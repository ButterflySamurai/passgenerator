{
    "name": "thenextweb/pass-generator",
    "description": "A Laravel package to create Apple Wallet (old Passbook) compatible tickets.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "The Next Web",
            "email": "support@thenextweb.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-zip": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "phpspec/phpspec": "~2.1",
        "orchestra/testbench": "~3.1",
    },
    "autoload": {
        "psr-4": {
            "PassGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PassGenerator\\Test\\": "tests"
        }
    }
}


